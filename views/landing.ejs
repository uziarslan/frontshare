                }, 3000);
            }
          }

          async function facebookLogin(verification_token){
            try {
                const result = await axios({
                  method: "GET",
                  url: `facebook/login?code=${verification_token}`,
                });
                if(result.status === 200 ){
                  const email = result.data.data.user.email
                  var credentials = JSON.parse(
                  localStorage.getItem("remembered_credential") || "[]"
                  );
                  if (!credentials.includes(email)) {
                    credentials.push(email);
                    localStorage.setItem(
                      "remembered_credential",
                      JSON.stringify(credentials)
                    );
                  }
                  var customAlert = document.createElement("div");
                  customAlert.classList.add(
                    "alert",
                    "alert-success",
                    "alert-dismissible",
                    "fade",
                    "show"
                  );
                  customAlert.style.bottom = "20px";
                  customAlert.style.right = "20px";
                  customAlert.style.maxWidth = "400px";
                  customAlert.style.fontSize = "14px";
                  customAlert.innerHTML = `
                    <h6 class="mt-2">Successfully logged in !</h6>
                  `;
                  document.getElementById("auth-alert").appendChild(customAlert);

                  setTimeout(function () {
                    customAlert.remove();
                    window.location.href = "/feed";
                  }, 3000);
                }
              }
            catch (error) {
              var customAlert = document.createElement("div");
              customAlert.classList.add(
                "alert",
                "alert-danger",
                "alert-dismissible",
                "fade",
                "show"
              );
              customAlert.style.bottom = "20px";
              customAlert.style.right = "20px";
              customAlert.style.maxWidth = "400px";
              customAlert.style.fontSize = "14px";
              customAlert.innerHTML = `
                <h6 class="mt-2">${error.response.data.error}</h6>
              `;
              document.getElementById("auth-alert").appendChild(customAlert);

              setTimeout(function () {
                customAlert.remove();
              }, 3000);            
            }
          }

          async function updatePassword(reset_token,newPassword, confirmNewPassword) {
            var newPasswordError = document.getElementById("password-error");
            var confirmPasswordError = document.getElementById("confirm-password-error");
            try {
              const result = await axios({
                method: "PATCH",
                url: `/api/v1/users/resetPassword/${reset_token}`,
                data: {
                  newPassword,
                  confirmNewPassword,
                },
              });
              if (result.hasOwnProperty('status') && result.status === 200) {
                $('#resetPasswordModal').modal('hide');
                var alertContainer = document.createElement("div");
                alertContainer.style.position = "fixed";
                alertContainer.style.top = "20px";
                alertContainer.style.right = "20px";
                alertContainer.style.zIndex = "9999"; // Ensure it appears above other elements
                alertContainer.style.maxWidth = "400px";

                // Create the alert element
                var customAlert = document.createElement("div");
                customAlert.classList.add("alert", "alert-success", "alert-dismissible", "fade", "show");
                customAlert.style.fontSize = "14px";
                customAlert.innerHTML = `
                  <h6 class="mt-2">Password is updated successfully!</h6>
                  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                `;

                alertContainer.appendChild(customAlert);
                document.body.appendChild(alertContainer);
                $('#exampleModalLogin').modal('show');


                setTimeout(function () {
                  alertContainer.remove();
                }, 5000);
                
              } else {
                var alertContainer = document.createElement("div");
                alertContainer.style.position = "fixed";
                alertContainer.style.top = "20px";
                alertContainer.style.right = "20px";
                alertContainer.style.zIndex = "9999"; // Ensure it appears above other elements
                alertContainer.style.maxWidth = "400px";

                // Create the alert element
                var customAlert = document.createElement("div");
                customAlert.classList.add("alert", "alert-success", "alert-dismissible", "fade", "show");
                customAlert.style.fontSize = "14px";
                customAlert.innerHTML = `
                  <h6 class="mt-2">Please enter a valid password</h6>
                  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                `;

                alertContainer.appendChild(customAlert);
                document.body.appendChild(alertContainer);

                setTimeout(function () {
                  alertContainer.remove();
                }, 5000);
              }
            } catch (error) {
              if (error.response.status == 400){
                newPasswordError.textContent = '';
                confirmPasswordError.textContent = '';
                confirmPasswordError.textContent = "Passwords do not match"
              }
              else if(error.response.status == 422){
                newPasswordError.textContent = '';
                confirmPasswordError.textContent = '';
                confirmPasswordError.textContent = "Password must be 8 characters long with atleast one capital, one digit and one symbol(! @ # $ %)"

              }
            }
          }

    function validatePassword() {
      var newPassword = document.getElementById("reset-password").value;
      var confirmPassword = document.getElementById("confirm-reset-password").value;
      var newPasswordError = document.getElementById("password-error");
      var confirmPasswordError = document.getElementById("confirm-password-error");

      newPasswordError.textContent = ''; // Clear previous error message
      confirmPasswordError.textContent = ''; // Clear previous error message

      // Check if both fields are at least 8 characters long
      if (newPassword.length < 8 || confirmPassword.length < 8) {
        newPasswordError.textContent = "Passwords must be at least 8 characters long";
        return false; // Prevent form submission
      }

      // Check if the passwords match
      if (newPassword !== confirmPassword) {
        confirmPasswordError.textContent = "Passwords do not match";
        return false; // Prevent form submission
      }
      
      // If validation passes, the form will be submitted
      return true;
    }
