              var alertContainer = document.createElement("div");
              alertContainer.style.position = "fixed";
              alertContainer.style.top = "20px";
              alertContainer.style.right = "20px";
              alertContainer.style.zIndex = "9999"; // Ensure it appears above other elements
              alertContainer.style.maxWidth = "400px";

              // Create the alert element
              var customAlert = document.createElement("div");
              customAlert.classList.add("alert", "alert-success", "alert-dismissible", "fade", "show");
              customAlert.style.fontSize = "14px";
              customAlert.innerHTML = `
                <h6 class="mt-2">The link is expired or Invalid!</h6>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              `;

              alertContainer.appendChild(customAlert);
              document.body.appendChild(alertContainer);

              setTimeout(function () {
                alertContainer.remove();
              }, 5000);
            }
