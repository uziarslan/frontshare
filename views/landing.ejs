    <script>
      // Define an array of options to be typed out
      const options = ["Socials", "Products", "Services", "Content", "News"];
      let currentOptionIndex = 0;
      // Function to simulate typing effect
      function typeEffect(text, delay) {
        let index = 0;
        const typingInterval = setInterval(() => {
          if(document.getElementById("typing-effect")) {
            document.getElementById("typing-effect").textContent = text.slice(
              0,
              ++index
            );}
          if (index >= text.length) {
            clearInterval(typingInterval);
            setTimeout(nextOption, 1000);
          }
        }, delay);
      }
      function nextOption() {
        currentOptionIndex = (currentOptionIndex + 1) % options.length;
        const currentOption = options[currentOptionIndex];
        typeEffect(currentOption, 100);
      }
      // Call the typing effect for the first option
      typeEffect(options[currentOptionIndex], 100);
    </script>

