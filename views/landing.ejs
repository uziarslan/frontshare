        async function confirmToken(reset_token) {
          try {
            const result = await axios({
              method: "POST",
              url: `/api/v1/users/checkValidity/${reset_token}`,
            });
            if (result.hasOwnProperty('status') && result.status === 200) {
              $('#resetPasswordModal').modal('show');
              
            } else {
