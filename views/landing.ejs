    <script>
      // Check if the browser supports local storage
      if (typeof Storage !== "undefined") {
        // Retrieve the saved email and password from local storage
        var savedEmail = localStorage.getItem("email");
        var savedPassword = localStorage.getItem("password");

        // If the saved email and password exist, populate the form fields
        if (savedEmail && savedPassword) {
          document.getElementById("email").value = savedEmail;
          document.getElementById("password").value = savedPassword;
          document.getElementById("modal-checkbox").checked = true;
        }

        // Add an event listener to the checkbox
        document
          .getElementById("modal-checkbox")
          .addEventListener("change", function () {
            // If the checkbox is checked, save the email and password to local storage
            if (this.checked) {
              var email = document.getElementById("email").value;
              var password = document.getElementById("password").value;
              localStorage.setItem("email", email);
              localStorage.setItem("password", password);
            } else {
              // If the checkbox is unchecked, remove the saved email and password from local storage
              localStorage.removeItem("email");
              localStorage.removeItem("password");
            }
          });
      }
